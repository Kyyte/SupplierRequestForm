<core:FragmentDefinition xmlns="sap.m" xmlns:f="sap.f" xmlns:core="sap.ui.core">
	<ResponsivePopover placement="Bottom" showHeader="false">
		<f:ProductSwitch change="fnChange"
			items="{
				path: 'switcher>/items'
			}">
			<f:items>
				<f:ProductSwitchItem src="{switcher>src}" title="{switcher>title}" subTitle="{switcher>subTitle}" targetSrc="{switcher>targetSrc}" target="{switcher>target}"/>
			</f:items>
		</f:ProductSwitch>
	</ResponsivePopover>
</core:FragmentDefinition>